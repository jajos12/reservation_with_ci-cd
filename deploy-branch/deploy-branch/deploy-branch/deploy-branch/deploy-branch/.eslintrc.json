{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": "off"
    }
} 